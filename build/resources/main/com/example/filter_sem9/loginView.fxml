<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.controller.LoginController" prefHeight="400.0" prefWidth="600.0">
    <Label text="Id" layoutX="100" layoutY="50"/>
    <Label text="Password" layoutX="100" layoutY="100"/>
    <TextField fx:id="userIdField" layoutX="160" layoutY="50" maxWidth="140"/>
    <TextField fx:id="passwordField" layoutX="160" layoutY="100" maxWidth="140"/>
    <Button fx:id="loginButton" text="Login" layoutX="200" layoutY="170"/>
</AnchorPane>